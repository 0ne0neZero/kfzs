<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wishare.finance.domains.voucher.repository.mapper.VoucherInferenceRecordMapper">

  <resultMap id="VoucherRuleMap" type="com.wishare.finance.domains.voucher.entity.VoucherInferenceRecordE">
    <result property="id" column="id" jdbcType="INTEGER"/>
    <result property="chargeItemId" column="charge_item_id" jdbcType="INTEGER"/>
    <result property="chargeItemName" column="charge_item_name" jdbcType="INTEGER"/>
    <result property="eventType" column="event_type" jdbcType="VARCHAR"/>
    <result property="voucherRuleId" column="voucher_rule_id" jdbcType="VARCHAR"/>
    <result property="voucherRuleName" column="voucher_rule_name" jdbcType="VARCHAR"/>
    <result property="voucherSystem" column="voucher_system" jdbcType="VARCHAR"/>
    <result property="successState" column="success_state" jdbcType="INTEGER"/>
    <result property="debitAmount" column="debit_amount" jdbcType="INTEGER"/>
    <result property="creditAmount" column="credit_amount" jdbcType="VARCHAR"/>
    <result property="deleted" column="deleted" jdbcType="INTEGER"/>
    <result property="tenantId" column="tenant_id" jdbcType="VARCHAR"/>
    <result property="creatorName" column="creator_name" jdbcType="VARCHAR"/>
    <result property="creator" column="creator" jdbcType="VARCHAR"/>
    <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
    <result property="operatorName" column="operator_name" jdbcType="VARCHAR"/>
    <result property="operator" column="operator" jdbcType="VARCHAR"/>
    <result property="gmtModify" column="gmt_modify" jdbcType="TIMESTAMP"/>
    <result property="name" column="name" jdbcType="VARCHAR"/>
    <result property="remark" column="remark" jdbcType="VARCHAR"/>
    <result property="voucherIds" column="voucher_ids" jdbcType="VARCHAR"/>
  </resultMap>

  <!--分页查询账簿列表-->
  <select id="queryPage" resultType="com.wishare.finance.domains.voucher.entity.VoucherInferenceRecordE">
    SELECT
      v.id,
      v.charge_item_id,
      v.charge_item_name,
      v.event_type,
      v.voucher_rule_id,
      v.voucher_rule_name,
      v.voucher_system,
      v.success_state,
      v.debit_amount,
      v.credit_amount,
      v.voucher_ids,
      v.deleted,
      v.tenant_id,
      v.creator_name,
      v.creator,
      v.gmt_create,
      v.operator_name,
      v.operator,
      v.gmt_modify
    FROM
      voucher_inference_record v
      ${ew.customSqlSegment}
  </select>
</mapper>
