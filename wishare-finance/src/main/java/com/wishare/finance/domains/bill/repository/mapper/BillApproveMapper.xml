<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wishare.finance.domains.bill.repository.mapper.BillApproveMapper">

    <select id="selectBillApproveEList" resultType="com.wishare.finance.domains.bill.entity.BillApproveE">
        select
        *
        from bill_approve b
        where
            b.operation_id = #{operationId}
          and b.sup_cp_unit_id = #{supCpUnitId}
    </select>
</mapper>
