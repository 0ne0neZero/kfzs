<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wishare.finance.domains.voucher.repository.mapper.VoucherBusinessDetailMapper">
    <update id="upAccountBookId">
        update voucher_business_detail set account_book_id= #{accountBookId} where voucher_id=#{voucherId};
    </update>



    <select id="fillInAccountBookId" resultType="com.wishare.finance.apps.model.voucher.vo.VoucherMiniV">
        select id,account_book_id from voucher_info
    </select>

</mapper>
