<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wishare.contract.domains.mapper.merchant.MerchantMapper">

    <select id="queryCountByToday" resultType="java.lang.Long">
        SELECT COUNT( * )
        FROM merchant
        where gmtCreate between #{startTime} and #{endTime}
        <if test="tenantId != null and tenantId != ''">
            and tenantId = #{tenantId}
        </if>
    </select>

</mapper>
